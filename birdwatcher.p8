isstart = false 
startishovered = false 

cloud_pos = {x = -100, y = 31}
nextbounce = 3 
nextclick = 1

function drawstartscreen()
		map(49,0,0,0,16,16)
		map(33,0,0,0,16,16)

		cloudoffsetx = 0 
		
		if cloud_pos.x < 0 then 
			cloudoffsetx = abs(cloud_pos.x)
		end 	
		sspr(cloudoffsetx ,65,80 - cloudoffsetx,54,cloud_pos.x + cloudoffsetx,cloud_pos.y)

	if cloud_pos.x == 20 then 
	startindex = 138 
	if startishovered then 
	startindex = 170
	end 
	spr(startindex,45,85,4,2 )
	end
end

function updatestart()
 if c.x > 35 
 and c.x < 75 
 and c.y >80 
 and c.y < 95 
 and cloud_pos.x == 20
 then 
  startishovered = true 
 else 
 	startishovered = false 
 end

	if btn(4) and startishovered then
		isstart = false
	end
	
	nextbounce -= 1
	nextclick -= 1
	
	if nextbounce == 0 then
	 nextbounce = 10 + flr(rnd(3)) 
		cloud_pos.y += flr(rnd(3)) + -1
	end
	
	if nextclick == 0 then 
		nextclick = 1 + flr(rnd(2))
		cloud_pos.x += 1
	end
	
	cloud_pos.x = min(cloud_pos.x, 20)
 cloud_pos.y = min(cloud_pos.y, 37)
 cloud_pos.y = max(cloud_pos.y, 25)

end

