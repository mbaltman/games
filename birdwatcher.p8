 b = {x=10,y=10,dx=1,dy=0}
	
	b_s_index = 1
	
	b_sprite = {64,65,66,65}
  
	b_sprite_offset = {0,16, 32, 48, 3, 19, 35, 51, 6}
	
	b_movement = {basex = 1, 
															xrange = 1, 
															basey = 1, 
															yrange = 1}
	
	
	b_movmentperlevel =
 {{1,1,1,1},
 {1,2,1,2},
 {2,2,2,2},
 {1,5,1,1},
 {2,1,2,1},
 {2,1,2,5},
 {2,5,2,1},
 {2,3,2,3},
 {4,2,4,2}
 }
	bird_startposoition = {
	{x=10,y=10},
	{x=40,y=40},
	{x=30,y=30},
	{x=20,y=80},
	{x=70,y=100},
	{x=100,y=20},
	{x=60,y=100},
	{x=5,y=100},
	{x=100,y=20}}
	
	
	function updatebird()
	
	 b.x += b.dx
		b.y += b.dy
		b_s_index += 0.3
		
		xvelocity = b_movement.basex + flr(rnd(b_movement.basex))
		yvelocity = b_movement.basey + flr(rnd(b_movement.basey))
		flipsign = -1 + rnd(2)
		
		if b.x > 128 then 
			b.dx = -1 * xvelocity 
			b.dy = flipsign * yvelocity
		end
		
		if b.x < 0 then
			b.dx = xvelocity
			b.dy = flipsign * yvelocity
		end
		
		if b.y > 128 then 
			b.dy = -1 * yvelocity
		end
		
		if b.y < 0 then
			b.dy = yvelocity
		end
	end
	
	function drawbird()
		curr_s_index = flr(b_s_index)%4 + 1
		curr = b_sprite[curr_s_index] + b_sprite_offset[level]
		spr(curr,b.x, b.y, 1,1 , b.dx < 0,false)	  
	end
