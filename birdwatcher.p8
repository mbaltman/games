-- level manager 

level = 1

function drawlevel()
	color(14)
	if gameover then 
 	spr(240, 5,13,13,1)
	end
	print("lv."..level, 5, 5)

end

function drawmap()
			if level < 4 or level > 7then 
				pal(1,1)
				pal(3,3)
				pal(11,11)
			else
				pal(1,2)
				pal(3,8)
				pal(11,9)
			end
			
			palt(15, true)
			if level > 7 then 
				map(0,16,0,0,16,16)
			else 
				map(0,0,0,0,16,16)
			end 
			
			pal(1,1)
			pal(3,3)
			pal(11,11)
			palt(15, true)
end

function increaselevel()

-- check if bird was captured
		if b.x > c.x 
		and b.x < c.x + 16 
		and b.y > c.y 
		and b.y < c.y + 16 
		and level < 10
		then 
			level += 1
			level = min(level, 9)
		 position = bird_startposoition[level]
		 b.x = position.x 
		 b.y = position.y 
		 movement = b_movmentperlevel[level]
		 b_movement.basex = movement[1]
		 b_movement.xrange = movement[2]
		 b_movement.basey = movement[3]
		 b_movement.yrange = movement[4]
		 
		 if level == 9 then 
		 	gamecompleted()
		 end
		end
end
	

