version 42
__lua__
	function _init()
		 palt(0b0000000000000001)
	end
	
	function _update()
	 if gameover then 
	 	updategamecompleted()
	 end
	 
	 if not(showphotobook) and not(isstart) then
		 updatebird()
		end
		
		if isstart then 
		updatestart()
		end
		
		updatecamera()
	end
	
	function _draw()
	 if isstart then 
	 	drawstartscreen()
	 elseif showphotobook then 
	  drawphotoalbum()
	  drawnavbutton()
			updatephotobook()
	 else 
		 	-- draw map
		 drawmap()
			drawlevel()
			-- draw bird
		 drawbird()
		 drawnavbutton()
			updatephotobook()
	 end 
	 
		drawcamera()
		
		if not isstart and not showphotobook then 
			drawflash()
		end
		
		if gameoverisanimating then 
			drawgamecompleted()
		end
end
