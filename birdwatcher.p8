--take photos

ishovered = false 
levelbeinghovered = 0 

showphotobook = false 

photo = {}

flashanimation = {x = 0, 
																		y = 0, 
																		frame =7}
flashssprites = {202,202,203,203,205,205,205}
function updatephotobook()
 ishovered = c.x<20 and c.y>105
 
 if btnp(4) then 
	 if ishovered then 
	 	showphotobook = not showphotobook
	 elseif not showphotobook then
	 	takephoto()
	 elseif showphotobook and levelbeinghovered != 0 then
	 	level = levelbeinghovered
	 	levelbeinghovered = 0
	 end
 end
 
 if flashanimation.frame > 0 then
 	flashanimation.frame += -1
 end
end

function drawnavbutton()
 spriteindex = 110 
 if not ishovered then 
 	spriteindex += 1
 end
 
 if showphotobook then 
 spriteindex += 16
 end
 
 spr(spriteindex,2,117) 
end

function takephoto()
		photo[level] = {}
		for x = 1,16 do 
	 	photo[level][x] = {}
			for y = 1,16 do 
   	photo[level][x][y] = pget(c.x + x - 1 , c.y + y - 1)
			end
		end
		flashanimation.x = c.x+8
		flashanimation.y = c.y+8
		flashanimation.frame = 7
		increaselevel()
end

function drawphotoalbum()
			map(17,0,0,0,16,16)
			levelbeinghovered = 0
			for i = 1 ,#photo do
				xoffset = 8 + (((i-1)% 3 ) * 40) 
				yoffset = 13 + (flr((i-1)/3) * 40) 
		  
		  if ceil(i / 3) % 2 == 0 then 
		  xoffset += 8
		  end
		  
		   -- check if this square is currenlty being hovered
		  if c.x + 5 > xoffset 
		  and c.x - 5 < xoffset + 16 
		  and c.y + 5 > yoffset 
		  and c.y - 5 < yoffset + 16 then 
		  	levelbeinghovered = i
		  end
		  
				for x = 1,16 do 
					for y = 1,16 do 
						pset(x + xoffset ,y + yoffset,photo[i][x][y])
					end
				end
			end
			drawlevelandhover()
end

function drawlevelandhover()
	xoffset = 8 + (((level-1)% 3 ) * 40) 
	yoffset = 13 + (flr((level-1)/3) * 40) 
	if ceil(level / 3) % 2 == 0 then 
		xoffset += 8
	end
	
	rect(xoffset, yoffset, xoffset + 16,yoffset + 16,10)
	
	xoffset = 8 + (((levelbeinghovered-1)% 3 ) * 40) 
	yoffset = 13 + (flr((levelbeinghovered-1)/3) * 40) 
	if ceil(levelbeinghovered / 3) % 2 == 0 then 
		xoffset += 8
	end
	rect(xoffset, yoffset, xoffset + 16,yoffset + 16,9)
end

function drawflash()
		flashsprite = flashssprites[flashanimation.frame]
		
		if flashsprite == 205 then		
			spr(205, 
			flashanimation.x - 12,
			flashanimation.y -12,
			3,
			3)
		elseif flashsprite == 203 then
			spr(203, 
			flashanimation.x - 8,
			flashanimation.y -8,
			2,
			2)
		elseif flashsprite == 202 then
			spr(202, 
			flashanimation.x - 4,
			flashanimation.y -4,
			1,
			1)
		end
		
end
